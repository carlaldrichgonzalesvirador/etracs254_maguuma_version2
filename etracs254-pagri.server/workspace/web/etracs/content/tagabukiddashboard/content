<script type="text/javascript"> 
\$(function () {

      date_time("time");
      \$.get("http://localhost:8072/osiris3/json/etracs25/TagabukidDashboardService.dash");
      \$.get("https://192.168.50.2:8999/webservice/assistancestatus");
 
      var qsock = Socket.create('tagabukidpoll'); 
      qsock.handlers.h = function( o ) { 
         
          fillpto(o.data.ptodash);
          fillsubay(o.data.subay);
          fillsandgravel(o.data.sandgravel);
          window.console.log( o ); 
      } 
      qsock.start(); 

     
      var connection = \$.hubConnection('https://192.168.50.2:8999/webservice'); 
      var sessionProxy = connection.createHubProxy('AIFCSTransac');

      sessionProxy.on('joined', function (item) {
          alert(item);
      });

      sessionProxy.on('assistamt', function (data) {
      //code nimo diri bay
          fillaifcs(data);
          fillaifcs2(data);
          fillaifcs3(data);
      });

      connection.start().done(function () {
          //setCookie("srconnectionid", connection.id);
      });

      
      function fillpto(o){
         \$.each(o,function(k,v){
              \$('#'+v.label).html( v.total );
          });
      }

      function fillaifcs(o){

          o = jQuery.parseJSON(o);
          data = o.map(function(a) {return a.data;});
          var d = [];
          var total = 0;
          for (var i = 0; i < data.length; i++) {
            var amount = 0;
            // alert(JSON.stringify(data[i]));
            for (var x = 0; x < data[i].length; x++) {
                // alert(JSON.stringify(data[i][x].type));
                if(data[i][x].type == "BURIAL"){
                  amount = data[i][x].amt;
                }  
            }
            total += amount;
            d.push(amount);
          }
       
          var data = {
            labels: o.map(function(a) {return a.Key;}),
            datasets: [
              {
                label: "TOTAL RELEASE AS OF TODAY (CY2016) = " + format2(total,"P"),
                backgroundColor: "#00a65a",
                borderColor: "#00a65a",
                data: d
              }
            ]
          };
          var lineChartCanvas = \$("#line1Chart").get(0).getContext("2d");
          var myLineChart = new Chart(lineChartCanvas, {
              type: 'line',
              data: data,
              options: {
                    responsive:true,
                    maintainAspectRatio: false,
                    events: false,
              }
          });
      }

      function fillaifcs2(o){

          o = jQuery.parseJSON(o);
          data = o.map(function(a) {return a.data;});
          var d = [];
          var total = 0;
          for (var i = 0; i < data.length; i++) {
            var amount = 0;
            for (var x = 0; x < data[i].length; x++) {
                if(data[i][x].type == "MISCELLANEOUS"){
                  amount = data[i][x].amt;
                }  
            }
            total += amount;
            d.push(amount);
          }
       
          var data = {
            labels: o.map(function(a) {return a.Key;}),
            datasets: [
              {
                label: "TOTAL RELEASE AS OF TODAY (CY2016) = " +  format2(total,"P"),
                backgroundColor: "#00c0ef",
                borderColor: "#00c0ef",
                data: d
              }
            ]
          };
          var lineChartCanvas = \$("#line2Chart").get(0).getContext("2d");
          var myLineChart = new Chart(lineChartCanvas, {
              type: 'line',
              data: data,
              options: {
                    responsive:true,
                    maintainAspectRatio: false,
                    events: false,
              }
          });
      }

      function fillaifcs3(o){

          o = jQuery.parseJSON(o);
          data = o.map(function(a) {return a.data;});
          var db = [];
          var totalb = 0;
          for (var i = 0; i < data.length; i++) {
            var amount = 0;
            for (var x = 0; x < data[i].length; x++) {
                if(data[i][x].type == "BURIAL"){
                  amount = data[i][x].amt;
                }  
            }
            totalb += amount;
            db.push(amount);
          }

          var dm = [];
          var totalm = 0;
          for (var i = 0; i < data.length; i++) {
            var amount = 0;
            for (var x = 0; x < data[i].length; x++) {
                if(data[i][x].type == "MISCELLANEOUS"){
                  amount = data[i][x].amt;
                }  
            }
            totalm += amount;
            dm.push(amount);
          }
       
       
          var data = {
            labels: o.map(function(a) {return a.Key;}),
            datasets: [
              {
                label: "BURIAL:" +  format2(totalb,"P"),
                backgroundColor: "#00a65a",
                borderColor: "#00a65a",
                data: db
              },
              {
                label: "MISCELLANEOUS:" +  format2(totalm,"P"),
                backgroundColor: "#00c0ef",
                borderColor: "#00c0ef",
                data: dm
              }
            ]
          };
          var lineChartCanvas = \$("#line3Chart").get(0).getContext("2d");
          var myLineChart = new Chart(lineChartCanvas, {
              type: 'line',
              data: data,
              options: {
                    responsive:true,
                    maintainAspectRatio: false,
                    events: false,
                    scales: {
                        yAxes: [{
                            stacked: true
                        }]
                    }
              }
          });
      }
     
     function fillsandgravel(o){
           var total = o.map(function(a) {return a.amount;}).reduce(function(a, b) { return a + b; }, 0);
           var data = {
            labels: o.map(function(a) {return a.month;}),
            datasets: [
              {
                label: "Total Amount As of Date (CY 2016) : " +format2(total,"P"),
                backgroundColor: "#dd4b39",
                borderColor: "#dd4b39",
                data: o.map(function(a) {return a.amount;})
              }
            ]
          };
          var barChartCanvas = \$("#bar1Chart").get(0).getContext("2d");
          var myBarChart = new Chart(barChartCanvas, {
              type: 'bar',
              data: data,
              options: {
                    responsive:true,
